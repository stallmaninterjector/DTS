
public class CaseyBats {
public static void main(String[] args) {
	System.out.println("  ,      ,");
	System.out.println("./(       )\\.");
	System.out.println(")  \\/\\_/\\/  (");
	System.out.println("\')  (^Y^)  (\'");
	System.out.println(" \'),-(~)-,(\'");
	System.out.println("     \'\"\'");
	System.out.println("");
	System.out.println("=,    (\\_/)    ,=");
	System.out.println(" /`-\'--(\")--\'-\'\\");
	System.out.println("");
	System.out.println("");
	System.out.println("");
	
}
}
